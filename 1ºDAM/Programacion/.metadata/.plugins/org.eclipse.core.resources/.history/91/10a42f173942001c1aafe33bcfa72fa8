/* //Ejercicio 2
import java.util.Scanner;

public class Tema2 {
	public static void main(String[] args) {
		int a;
		int b;
		int c;
		Scanner scan = new Scanner(System.in);
		System.out.println("Introduzca valor a: ");
		a = scan.nextInt();
