/* //Ejercicio3
import java.util.Scanner;

public class Tema2 {
	public static void main(String[] args) {
		int prec;
		double e;
		Scanner scan = new Scanner(System.in);
		System.out.println("Introduzca la precisión: ");
		prec = scan.nextInt();
		while (prec < 0) {
			System.out.println("Introduzca un positivo: ");
			prec = scan.nextInt();
		}
		scan.close();
		e = exp(prec);
		System.out.println(e);
	}

	public static double exp(int a) {
		double x = 1;
		double aux;
		for (int i = 1; i < a; i++) {
			aux = factorial(i);
			x = x + 1 / aux;
		}
		return x;
	}

	public static double factorial(int b) {
		double fact = 1;
		for (int i = b; i > 1; i--) {
			fact = fact * i;
		}
		return fact;
	}
}
*/

/*
//Ejercicio4a
import java.util.Scanner;

public class Tema2 {
	public static void main(String[] args) {
		int cont;
		char x;
		String cad;
		Scanner scan = new Scanner(System.in);
		System.out.println("Introduzca un carácter:");
		x = scan.next().charAt(0);
		System.out.println("Introduce una secuencia de caracteres:");
		cad = scan.next();
		scan.close();
		cont = recuento(x, cad);
		System.out.println("El caracter " + x + " se repite " + cont + " veces");
	}

	public static int recuento(char a, String b) {
		int cont = 0;
		for (int i = 0; i < b.length(); i++) {
			if (b.charAt(i) == '.') {
				return cont;
			}
			if (b.charAt(i) == a) {
				cont++;
			}
		}
		return cont;
	}
}
*/


