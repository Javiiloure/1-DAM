/*
//Ejercicio5
public class Tema2 {
	public static void main(String[] args) {
		int x;
		Scanner scan = new Scanner(System.in);
		System.out.println("Introduzca un entero: ");
		x = scan.nextInt();
		scan.close();
		x = inversión(x);
		System.out.println("El número invertido es: " + x);
	}

	public static int inversión(int a) {
		String rev = "";
		String num = Integer.toString(a);
		for (int i = num.length(); i < 0; i--) {
			rev += num.charAt(i);
		}
		a = Integer.parseInt(rev);
		return a;
	}
}
*/

/*
//Ejercicio6
public class Tema2 {
	public static void main(String[] args) {
		int a;
		int b;
		boolean amistad;
		Scanner scan = new Scanner(System.in);
		System.out.println("Introduzca un primer número: ");
		a = scan.nextInt();
		System.out.println("Introduzca un segundo número: ");
		b = scan.nextInt();
		scan.close();
		amistad = amigos(a, b);
		if(amistad == true) {
			System.out.println("Los números " + a + " y " + b + " son amigos.");
		}
		else if(amistad == false) {
			System.out.println("Los números " + a + " y " + b + " no son amigos.");
		}
	}

	public static boolean amigos(int a, int b) {
		int sum1 = 0;
		int sum2 = 0;
		boolean amistad = false;
		for (int i = 1; i < a; i++) {
			if (a % i == 0) {
				sum1 += i;
			}
		}
		if(b != sum1) {
			return amistad;
		}
		for (int i = 1; i < b; i++) {
			if (b % 2 == 0) {
				sum2 += i;
			}
		}
		if (sum1 == b && sum2 == a) {
			amistad = true;
		}
		return amistad;
	}
}
*/