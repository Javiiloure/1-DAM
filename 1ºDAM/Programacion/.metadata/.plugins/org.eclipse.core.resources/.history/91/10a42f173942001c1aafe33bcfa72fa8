		b = scan.nextInt();
		System.out.println("Introduzca valor c: ");
		c = scan.nextInt();
		scan.close();
		System.out.println("El mayor valor es " + mayor(a, b, c));
	}

	public static int mayor(int a, int b, int c) {
		if (a >= b && a >= c) {
			return a;
		} else if (b >= a && b >= c) {
			return b;
		} else {
			return c;
		}
	}
