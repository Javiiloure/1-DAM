public class DNI {
	int dni;
	char letra;
	
	public DNI () {
		this.dni = 0;
		
	}
	
	public DNI (int dni, char letra) {
		this.dni = dni;
		this.letra = letra;
	}
	public void setdni(int dni) {
		this.dni = dni;
	}
	
	public int getdni() {
		return dni;
	}
	
	public void setletra(char letra) {
		this.letra = letra;
	}
}
